			form {
				max-width: 600px;
				margin: 0 auto;
				padding: 1rem;
				border: 1px solid #cccccc;
				border-radius: 5px;
			}
			form label {
				display: block;
				margin-bottom: 0.5rem;
				font-weight: 600;
			}
			form input,
			form select {
				margin-bottom: 1rem;
				padding: 0.5rem;
				border: 1px solid #ccc;
				border-radius: 3px;
				width: 100%;
				box-sizing: border-box;
				font-size: 1rem;
				font-weight: 400;
			}
			form button[type="submit"] {
				background-color: #007bff;
				color: #ffffff;
				border: none;
				border-radius: 3px;
				padding: 0.5rem 1rem;
				font-size: 1rem;
				cursor: pointer;
				transition: background-color 0.3s ease-in-out;
			}
			form button[type="submit"]:hover {
				background-color: #0069d9;
			}

